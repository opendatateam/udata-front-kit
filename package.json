    "@sentry/vue": "^10.17.0",
    "@unhead/vue": "^1.11.18",
    "@vueform/multiselect": "^2.6.11",
    "@vueform/slider": "^2.1.10",
    "@vueuse/core": "^12.4.0",
    "@vueuse/integrations": "^12.4.0",
