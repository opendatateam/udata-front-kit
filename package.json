    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^20.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "mimicry-js": "^1.3.0",
