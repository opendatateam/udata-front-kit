    "node": ">= 20.0.0",
    "npm": ">= 10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:single": "vitest run",
    "test:e2e": "node scripts/cypress-e2e.mjs",
